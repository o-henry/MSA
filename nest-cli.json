{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/main/src",
  "monorepo": true,
  "root": "apps/main",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/main/tsconfig.app.json"
  },
  "projects": {
    "main": {
      "type": "application",
      "root": "apps/main",
      "entryFile": "main",
      "sourceRoot": "apps/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main/tsconfig.app.json"
      }
    },
    "order": {
      "type": "application",
      "root": "apps/order",
      "entryFile": "main",
      "sourceRoot": "apps/order/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order/tsconfig.app.json"
      }
    },
    "ddd": {
      "type": "application",
      "root": "apps/ddd",
      "entryFile": "main",
      "sourceRoot": "apps/ddd/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ddd/tsconfig.app.json"
      }
    }
  }
}